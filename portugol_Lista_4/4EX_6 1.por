programa {
  funcao inicio() {
     //Construa um algoritmo que leia o preço de um produto (P) e apresente a mensagem: “Em promoção”, 
     //caso o preço seja maior ou igual a R$ 50,00 e menor ou igual a R$ 100,00. 
     //Caso contrário, deve apresentar a mensagem: “Preço Normal”.
 
      real valorProduto
     escreva("Digite o preço do produto:\t")
     leia(valorProduto)

    se (valorProduto<50.00)
      escreva("Preço Normal")
       
    senao se( valorProduto<=100)
      escreva("Em promoção")
    senao
      escreva("Preço Normal")
     
     
  
  }
}
