programa {
  funcao inicio() {
    //Escreva um programa que imprima a tabuada de um número digitado pelo usuário.
    
    inteiro tabuada, ate, i
    
    escreva("Digite a tabuada do numero\t ")
    leia(tabuada)
    escreva("até ")
    leia(ate)
    para(i=0;i<=ate ;i++){

   escreva("\n" ,tabuada," x ",i, "=" ,i*tabuada )
   
    }
  }
}
