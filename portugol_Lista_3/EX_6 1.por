programa {
  funcao inicio() {
    //Entrar com um número e imprimir a mensagem correta: é múltiplo de 3 ou não é múltiplo de 3. 
  
  real numero 

  escreva("Digite um numero: ")
  leia(numero)

  se (numero %3 ==0){
    escreva("É multiplo de três")
  }
  senao{
     escreva("Não é multiplo de três")
  }

  
  }
}
