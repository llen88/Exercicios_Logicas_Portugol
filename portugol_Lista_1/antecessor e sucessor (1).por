programa {
  funcao inicio() {
    //Elabore um programa em que o usuário entre com um número inteiro e o programa exiba seu antecessor e seu sucessor. 

 // variavel de entrada
 inteiro numero
 //variavel de saida
 inteiro antecessor, sucessor

 //entrada
 escreva ("Digite um número inteiro:\t")
 leia (numero)
 //processamento
 antecessor= (numero -1)
 sucessor= (numero +1)
 escreva("Antecessor: ", antecessor)
 escreva("\nSucessor: ", sucessor)


  }
}
