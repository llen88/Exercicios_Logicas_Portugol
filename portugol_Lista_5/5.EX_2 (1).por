programa {
  funcao inicio() {
   // Construa um algoritmo que leia o código de um livro (CL) e apresente a categoria do livro, conforme a tabela abaixo:
  
   inteiro codigo

    escreva(" DIGITE O CODIGO DO LIVRO:\t")
    leia (codigo)
   
    escolha(codigo)
    {
      caso 1:
        escreva("Ficção")
        pare
      caso 2:
        escreva("Não ficcção")
        pare
    
    }
  }
}